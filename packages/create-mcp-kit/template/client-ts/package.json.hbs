{
  "name": "{{projectName}}",
  "version": "0.0.0",
  "description": "{{projectName}}",
  "author": "zhensherlock",
  "type": "module",
  "license": "MIT",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "main": "build/index.js",
  "scripts": {
{{#if (includes plugins 'style')}}
    "lint": "npx eslint \"src/**/*.{ts,js}\"",
{{/if}}
    "build": "tsc --noEmit && cross-env NODE_ENV=production node scripts/build.js",
    "dev": "cross-env NODE_ENV=local concurrently \"tsc --noEmit --watch\" \"node scripts/dev.js\"",
{{#if (includes plugins 'vitest')}}
    "test": "vitest run",
    "coverage": "rimraf coverage && vitest run --coverage",
{{/if}}
{{#if (includes plugins 'changelog')}}
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -n changelog-option.js",
{{/if}}
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
{{#if (includes plugins 'commitlint')}}
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
{{/if}}
{{#if (includes plugins 'style')}}
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
{{/if}}
{{#if (includes plugins 'vitest')}}
    "@vitest/coverage-v8": "^3.2.4",
{{/if}}
{{#if (includes plugins 'changelog')}}
    "compare-func": "^2.0.0",
{{/if}}
    "concurrently": "^9.2.1",
{{#if (includes plugins 'changelog')}}
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
{{/if}}
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.9",
{{#if (includes plugins 'style')}}
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
{{/if}}
    "husky": "^9.1.7",
{{#if (includes plugins 'style')}}
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
{{/if}}
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
{{#if (includes plugins 'vitest')}}
    "vitest": "^3.2.4",
{{/if}}
    "rimraf": "^6.0.1"
  }
}
