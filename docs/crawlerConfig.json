{
  "index_name": "create-mcp-kit",
  "start_urls": [
    {
      "url": "https://my-mcp-hub.github.io/mcp-kit/",
      "tags": ["gh"]
    },
    {
      "url": "https://mcp-kit-docs.vercel.app/",
      "tags": ["vercel"]
    }
  ],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "",
        "default_value": "Documentation"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3",
      "lvl4": ".content h4",
      "lvl5": ".content h5",
      "content": ".content p, .content li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      }
    }
  },
  "selectors_exclude": ["aside", ".page-footer", ".next-and-prev-link", ".table-of-contents"],
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  },
  "js_render": true
}
